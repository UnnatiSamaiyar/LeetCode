class Solution {
public:
    string interpret(string command) {
        string s = "";
        int n = command.length();
        for(int i=0; i<n; i++){
            if(command[i] == 'G'){
                s += command[i];
                continue;
            }
            if(command[i] == '(' && command[i+1] == ')'){
                s += "o";
                i++;
            }
            if(command[i] == '(' && command[i+1] == 'a'){
                s += "al";
                i += 3;
            }
        }
        return s;
    }
};
